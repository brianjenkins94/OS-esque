{
  "name": "elegancss",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rollup --config --watch",
    "csscomb": "csscomb --config docs/css/.csscomb.json docs/css/style.scss docs/css/partials/*.scss",
    "cypress": "watch \"cypress run --spec cypress/integration/screenshot.spec.js && find docs/images/ -type f -mindepth 2 -exec mv \"{}\" docs/images/ \\; && rmdir docs/images/* &> /dev/null\" docs/css docs/js",
    "sassc": "sass --watch docs/css/style.scss docs/css/style.css --no-source-map",
    "serve": "npx npx http-server docs/ -c-1 -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianjenkins94/elegancss.git"
  },
  "author": "Brian Jenkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianjenkins94/elegancss/issues"
  },
  "homepage": "https://github.com/brianjenkins94/elegancss#readme",
  "dependencies": {
    "ts-node": "latest",
    "typescript": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "cypress": "latest",
    "eslint": "latest",
    "rollup": "latest",
    "rollup-plugin-typescript": "latest",
    "tslib": "latest",
    "watch": "latest"
  }
}
